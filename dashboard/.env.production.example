# ===================================
# DASHBOARD PRODUCTION ENVIRONMENT
# ===================================
# Copy this file to .env.production and update with your Railway URLs

# Backend API URL (REQUIRED)
# After deploying the backend service, copy its Railway URL here
# Example: https://equity-ai-trading-backend-production.up.railway.app
VITE_API_URL=https://your-backend-service.railway.app

# WebSocket URL (REQUIRED)
# Same as API URL but with wss:// protocol instead of https://
# Example: wss://equity-ai-trading-backend-production.up.railway.app
VITE_WS_URL=wss://your-backend-service.railway.app

# Use Mock Data (set to false for production)
VITE_USE_MOCK_DATA=false
